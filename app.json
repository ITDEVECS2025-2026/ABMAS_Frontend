{
  "expo": {
    "name": "Soil Monitor",
    "slug": "abmas-fe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./styles/assets/images/icon.png",
    "scheme": "abmasfe",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./styles/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ecslaboratory.abmasecsapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./styles/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./styles/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "844d1988-614c-4e42-bc5b-5d848aa47f29"
      }
    }
  }
}